local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/kav"))()
local Window = Library.CreateLib("Money Simulator Autofarm", "Synapse")
local Tab = Window:NewTab("Autofarming")
local Section = Tab:NewSection("Autofarming")
Section:NewButton("Anti-AFK", "Antiafk, And It Works", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/2dgeneralspam1/scripts-and-stuff/master/scripts/LoadstringypVvhJBq4QNz", true))()
end)
Section:NewButton("Autofarm OP", "Includes autofarm, And fillmoney", function()
while true do

wait (.1)

local A_1 = "DropOn"
local Event = game:GetService("ReplicatedStorage").GetMoney
Event:FireServer(A_1)
end
end)
Section:NewButton("FillMoney Enable With Autofarm", "FillMoney", function()
while true do

wait (.1)

local A_1 = "DropOn"
local Event = game:GetService("ReplicatedStorage").FillMoney
Event:FireServer(A_1)
end
end)
local Tab = Window:NewTab("AutoBuy")
local Section = Tab:NewSection("Buying Tab")
Section:NewButton("Power/More Money", "Makes it auto buy power.", function()
while true do

wait (.1)

local Event = game:GetService("ReplicatedStorage").UpgradePower
Event:FireServer()
end
end)
Section:NewButton("Bag", "More Storage", function()
while true do

wait (.1)

local Event = game:GetService("ReplicatedStorage").UpgradeBag
Event:FireServer()
end
end)
local Tab = Window:NewTab("Credits")
